{
  "$schema": "node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": false,
    "packageManager": "pnpm",
    "schematicCollections": [
      "angular-eslint"
    ]
  },
  "newProjectRoot": "projects",
  "projects": {
    "seed": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "@babel/runtime-corejs3/core-js/aggregate-error",
              "@braintree/sanitize-url",
              "base64-js",
              "classnames",
              "core-js-pure/features/instance/bind.js",
              "core-js-pure/features/object/assign.js",
              "css.escape",
              "deepmerge",
              "dompurify",
              "highlight.js/lib/languages/bash",
              "highlight.js/lib/languages/http",
              "highlight.js/lib/languages/javascript",
              "highlight.js/lib/languages/json",
              "highlight.js/lib/languages/powershell",
              "highlight.js/lib/languages/xml",
              "highlight.js/lib/languages/yaml",
              "ieee754",
              "immutable",
              "js-file-download",
              "lodash/camelCase",
              "lodash/constant",
              "lodash/debounce",
              "lodash/eq",
              "lodash/escapeRegExp",
              "lodash/find",
              "lodash/fp/assocPath",
              "lodash/fp/set",
              "lodash/get",
              "lodash/has",
              "lodash/identity",
              "lodash/isEmpty",
              "lodash/isFunction",
              "lodash/isPlainObject",
              "lodash/isString",
              "lodash/memoize",
              "lodash/merge",
              "lodash/omit",
              "lodash/reduce",
              "lodash/set",
              "lodash/some",
              "lodash/toLower",
              "lodash/toString",
              "lodash/upperFirst",
              "lodash/zipObject",
              "lowlight/lib/core",
              "minim",
              "process",
              "prop-types",
              "randexp",
              "react",
              "react-copy-to-clipboard",
              "react-debounce-input",
              "react-dom",
              "react-immutable-proptypes",
              "redux-immutable",
              "serialize-error",
              "short-unique-id",
              "unraw",
              "url-parse",
              "use-sync-external-store/with-selector.js",
              "xml-but-prettier",
              "zenscroll"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/@seed/styles"
              ]
            },
            "styles": [
              "src/styles/splash-screen.scss",
              "src/@seed/styles/tailwind.scss",
              "src/@seed/styles/themes.scss",
              "src/styles/vendors.scss",
              "src/@seed/styles/main.scss",
              "src/styles/styles.scss",
              "src/styles/tailwind.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "200kb",
                  "maximumError": "200kb"
                }
              ],
              "outputHashing": "all",
              "extractLicenses": false
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "seed:build:production"
            },
            "development": {
              "buildTarget": "seed:build:development",
              "host": "0.0.0.0"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "disableHostCheck": true,
            "host": "0.0.0.0",
            "proxyConfig": "proxy.conf.mjs"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  }
}
